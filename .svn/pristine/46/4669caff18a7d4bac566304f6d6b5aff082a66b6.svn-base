@charset "utf-8";

/* CSS Document */
ul {
    padding: 0;
    margin: 0
}
li {
    list-style: none;
    vertical-align: bottom
}
.w1000 {
    width: 1020px;
    margin: auto
}
.fl {
    float: left
}
.fr {
    float: right
}
.clear {
    clear: both
}
.b5 {
    height: 5px
}
.b10 {
    height: 10px
}
.b8 {
    height: 8px
}
.b15 {
    height: 15px
}
.b20 {
    height: 20px
}
.f12 {
    font-size: 12px
}
.f13 {
    font-size: 13px
}
.f14 {
    font-size: 14px
}
.f15 {
    font-size: 15px
}
.f16 {
    font-size: 16px
}
.radius8 {
    -moz-border-radius: 8px;
    -webkit-border-radius: 8px;
    border-radius: 8px
}
.radius5 {
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    border-radius: 5px
}
.top {
    background-color: #434343;
    height: 50px;
    padding: 6px 20px 0 20px
}
.top ul.list-inline {
    margin-top:5px;
    font-size: 0px;
    float:left;
}
.top .list-inline a {
    display: block;
    width: 102px;
    text-align: center;
    color: #fff;
    height: 28px;
    line-height: 25px;
    font-size:14px;
    position:relative;
}
.top .list-inline > li{
    padding:0px;
}

.top .list-inline .first a{background-image:url('../images/top_a1_act.png');background-size:100%;background-repeat:no-repeat;border-radius:15px 0px 0px 15px;cursor: pointer;}
.top .list-inline .second a{background-image:url('../images/top_a2.png');background-size:100%;background-repeat:no-repeat;border-radius:0px 15px 15px 0px;left: -16px;cursor: pointer;}
.top .logo {
    height: 24px;
    width:108px;
    float:left;
    background-image: url(../images/logo.png);
    background-repeat: no-repeat;
    background-position: center center;
    margin-top:7px;
    margin-right:85px;
    margin-left:40px;
}

.top .list-inline a:hover {
    color: #FFF;
}
.top .list-inline a.hover {
    color: #FFF;
}
.top .list-inline .first a:hover{background-image:url('../images/top_a1.png');}
.top .list-inline .second a:hover{background-image:url('../images/top_a2_act.png');}

.top .list-inline .first a.hover{background-image:url('../images/top_a1.png');}
.top .list-inline .second a.hover{background-image:url('../images/top_a2_act.png');}



.top ul.fr {
    margin:0px;
    line-height: 40px;
    font-size: 12px;
    margin-right:60px;
}
.top ul.fr li {
    float: right;
    margin-left: 45px;
    color: #fff
}
.top ul.fr li.sign_out a{color:#e60012;}
.top ul.fr li.sign_out a:hover{color:#fff;}

.top ul.fr li a {
    color: #fff
}
.top ul.fr li a:hover {
    color: #e60012
}
.sidebar {
    height:80px;
    background:#313131;
}
.sidebar .con ul{margin:0px;float:left;}
.sidebar .con ul li {
    line-height: 80px;
    text-align:center;
    float:left;
    width:217px;
    font-size: 16px;
    color:#e8e8e8;
    position:relative;
}
.sidebar .con ul li img{margin-right:7px;position:relative;top:-1px;}
.sidebar .con ul li.tit {
    background-image:url('../images/menu_til.png');
    background-size:100%;
    left:0px;
}
.sidebar .con ul li a {
    display: block;
    color:#e8e8e8;
}
.sidebar .con ul li a:hover::after,.sidebar .con ul li a.hover::after{
    content:'';
    position:absolute;
    width:50px;
    height:2px;
    left:50%;
    margin-left:-25px;
    bottom:0px;
    background:#e60012;
}
.sidebar .con ul li a.hover {
    color: #e8e8e8;
}
.sidebar .conLanmu ul li {
    line-height: 30px;
    height: 30px
}
.sidebar .conLanmu ul li.tit {
    line-height: 35px;
    height: 35px;
    background: #e6f2fb;
    border-top: solid 1px #b5cfd9;
    border-bottom: solid 1px #b5cfd9;
    font-size: 16px;
    font-weight: 700;
    padding-left: 15px
}
.sidebar .conLanmu ul li.tit a:hover {
    background: 0 0;
    color: #333
}
.sidebar .conLanmu ul li a {
    color: #333;
    display: block
}

.admin-bread {
    background: #1b1b1b;
    line-height: 40px;
    height: 40px;
    color:#fff;
/*    position:absolute;
    top:50px;
    width:100%;*/
}
.admin-bread span.fr{margin-right:80px;}
.admin-bread span.fr a{margin-left:50px;}
.admin-bread ul {
    padding: 0;
    margin: 0;
    margin-left:255px;
}
.admin-bread ul li {
    float: left;
    margin-right: 10px;
    color: #fff
}
.admin-bread a:hover, .admin-bread a:focus {
    color: #e60012;
    text-decoration: none;
}
.admin-bread a{
    color:#fff;
}
.content {
    padding: 20px
}
.zhichi {
    margin-top: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    border-top: solid 1px #b5cfd9;
    padding-left: 20px
}
.tab-diy-con {
    padding: 15px 0
}
.content .pag {
    background-color: #F7F7F7;
    border: 1px solid #E4E4E4;
    text-align: center;
    box-shadow: 0px 0px 10px #999;
    border-radius: 10px;
}
.content .pag a {
    display: inline-block;
    height: 20px;
    line-height: 20px;
    padding: 0 6px;
    border: 1px inset #CCC;
    margin: 0 3px;
    background-color: #FFF;
    color: #666;
    font-size: 12px
}
.content .pag a.Invalid, .content .pag a.present, .content .pag a:hover {
    border: 1px outset #CCC;
    background-color: #E8E8E8;
    color: #000
}
.zhichi {
    font-size: 12px;
}
.table > thead > tr > th, .table > tbody > tr > th, .table > tfoot > tr > th, .table > thead > tr > td, .table > tbody > tr > td, .table > tfoot > tr > td{
    padding:15px 10px;
    border-bottom: 1px dashed #434343;
    border-top:none;
    border:none;
}
.table tr{border-bottom: 1px dashed #434343;}
/*.table tr:first-child,.table tr:last-child{border:none;}*/
.panel-default > .panel-heading{
    background-color:#434343;
    color:#fff;
    padding:25px 15px;
    border-radius:10px 10px 0px 0px;
}
.panel{border:none;box-shadow:0px 0px 10px #999;border-radius:10px;overflow:hidden;}
.panel-default > .panel-heading.new_title .glyphicon{margin-right:10px;}
.table{padding:5px;}
.content .new_add{background:#be1414;color:#fff;border-radius:18.5px;margin-bottom:20px;}
.content .new_add:hover, .content .new_add:focus, .content .new_add.focus{color:#fff;box-shadow:0px 0px 5px #666;}
table{background:#fff;}
.table .active{color:#fff;border:none;}
.table .active td,.table .active tr{background-color:#434343 !important;padding:25px 0px;}
.new-btn-warning{background:#be1414;border-radius:15px;color:#fff;}
.new-btn-warning:hover, .new-btn-warning:focus, .new-btn-warning.focus{color:#fff !important;box-shadow:0px 0px 5px #666;}
.new-btn-default{border-radius:15px;border:1px solid #434343;color:#434343;}
.new-btn-default:hover, .new-btn-default:focus, .new-btn-default.focus{color:#434343;box-shadow:0px 0px 5px #666;}
.new-btn-primary{background:#00736d;border-radius:15px;color:#fff;}
.new-btn-primary:hover, .new-btn-primary:focus, .new-btn-primary.focus{color:#fff !important;box-shadow:0px 0px 5px #666;}
.new-btn-warning,.new-btn-primary,.new-btn-default{padding:1px 10px;}
.table tbody tr.active td{border:none;}
.menu_fan{float:right;height:80px;line-height:80px;color:#fff;padding-right:100px;}
.menu_fan a{color:#e60012;}
.new-btn-warning img, .new-btn-primary img, .new-btn-default img{width:18px;height:18px;margin-right:5px;}
.admin-bread .fr a img{margin-right:12px;position:relative;top:-2px;}
.border-table{border-left:10px solid #fff;border-right:10px solid #fff;}
.table .active td:last-child strong{border-right:none;}
.table strong{display:block;border-right:1px solid #959595;}
.border-left{border-left:15px solid #fff !important;box-sizing:border-box;}
.border-right{border-right:15px solid #fff !important;box-sizing:border-box;}
.new-form-inline{margin-bottom:5px;}
.new-form-inline .form-group input{line-height: 34px;padding:0px 15px;margin-left:15px;margin-right:15px;}
.new-form-inline .form-group button{margin-left:15px;}